package fileIO;

import java.io.FileInputStream;

public class FileInputStreamDemo {

	public static void main(String[] args) 
	{
		try
		{
			FileInputStream obj=new  FileInputStream("D:\\FileTest1.txt");
			int i=0;
			while((i=obj.read())!=-1)
			{
				System.out.print((char)i);
			}
			obj.close();
			}
		catch(Exception e)
		{
			System.out.println(e);
		}


	}

}
