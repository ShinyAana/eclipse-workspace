package oops;
class A
{
	public void display()
	{
		System.out.println("Class A display");
	}
}
public class AnonymousDemo {
	 A obj=new A()
			{
		public void display()
		{
			super.display();
			System.out.println("Anonymous demo display");
			
		}
			};

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A obj1=new A();
		obj1.display();

	}

}
