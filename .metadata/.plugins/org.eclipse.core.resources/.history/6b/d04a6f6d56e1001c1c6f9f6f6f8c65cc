package connectingFolksWeb;

public class PostDetails {
	 String userName;
     String shareTo;
     String subject;
     String message;
     int postId;
	PostDetails(String userName,String shareTo,String subject,String message,int postId)
	{
	this.userName=userName;
	this.shareTo=shareTo;
	this.subject=subject;
	this.message=message;
	this.postId=postId;
	
	}


}
