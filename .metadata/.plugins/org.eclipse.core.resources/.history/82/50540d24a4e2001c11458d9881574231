package extra;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class QueensAttack {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		List<List<Integer>> obstacles=new ArrayList<List<Integer>>();
		
		obstacles.add(new ArrayList<Integer>());
		obstacles.get(0).add(0,5);
		obstacles.get(0).add(1,5);
		obstacles.add(new ArrayList<Integer>(Arrays.asList(4,5)));
		obstacles.add(new ArrayList<Integer>(Arrays.asList(6,7)));
		
		System.out.println(obstacles);
		
	}

}
