package oops;
class Book
{
	public void updateDB()
	{
		for(int i=1;i<5;i++)
		{
		System.out.println("Updating DB");
		}
	}
}
class Num
{
	public void printNum()
	{
		for(int i=1;i<5;i++)
		{
		System.out.println(i);
		}
	}
}
public class MultithreadingDemo {

	public static void main(String[] args)
	{
	Book b=new Book();
	Num n=new Num();
	b.updateDB();
	n.printNum();
	}

}
